# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~
GET		  /health						    assessment.controllers.HealthController.health()
GET       /service/health                   assessment.controllers.HealthController.serviceHealth()

# ItemSet API's
POST      /itemset/v3/create                assessment.controllers.v3.ItemSetController.create()
GET       /itemset/v3/read/:identifier      assessment.controllers.v3.ItemSetController.read(identifier:String, fields:Option[String])
PATCH     /itemset/v3/update/:identifier    assessment.controllers.v3.ItemSetController.update(identifier:String)
POST      /itemset/v3/review/:identifier    assessment.controllers.v3.ItemSetController.review(identifier:String)
DELETE    /itemset/v3/retire/:identifier    assessment.controllers.v3.ItemSetController.retire(identifier:String)

# Assessment Item API's
POST      /assessment/v3/items/create         assessment.controllers.v3.AssessmentItemController.create()
GET       /assessment/v3/items/read/:id       assessment.controllers.v3.AssessmentItemController.read(id:String, ifields:Option[String], fields:Option[String])
PATCH     /assessment/v3/items/update/:id     assessment.controllers.v3.AssessmentItemController.update(id:String)
DELETE    /assessment/v3/items/retire/:id     assessment.controllers.v3.AssessmentItemController.retire(id:String)

# Question API's
POST      /question/v4/create                     assessment.controllers.v4.QuestionController.create()
GET       /question/v4/read/:identifier           assessment.controllers.v4.QuestionController.read(identifier:String, mode:Option[String], fields:Option[String])
GET       /question/v4/private/read/:identifier   assessment.controllers.v4.QuestionController.privateRead(identifier:String, mode:Option[String], fields:Option[String])
PATCH     /question/v4/update/:identifier         assessment.controllers.v4.QuestionController.update(identifier:String)
POST      /question/v4/review/:identifier         assessment.controllers.v4.QuestionController.review(identifier:String)
POST      /question/v4/publish/:identifier        assessment.controllers.v4.QuestionController.publish(identifier:String)
DELETE    /question/v4/retire/:identifier         assessment.controllers.v4.QuestionController.retire(identifier:String)
POST      /question/v4/import                     assessment.controllers.v4.QuestionController.importQuestion()
PATCH     /question/v4/system/update/:identifier  assessment.controllers.v4.QuestionController.systemUpdate(identifier:String)
POST      /question/v4/list                       assessment.controllers.v4.QuestionController.list(fields:Option[String])
POST      /question/v4/reject/:identifier         assessment.controllers.v4.QuestionController.reject(identifier:String)
POST      /question/v4/copy/:identifier           assessment.controllers.v4.QuestionController.copy(identifier:String, mode:Option[String])

# QuestionSet API's
POST      /questionset/v4/create                     assessment.controllers.v4.QuestionSetController.create()
GET       /questionset/v4/read/:identifier           assessment.controllers.v4.QuestionSetController.read(identifier:String, mode:Option[String], fields:Option[String])
GET       /questionset/v4/private/read/:identifier   assessment.controllers.v4.QuestionSetController.privateRead(identifier:String, mode:Option[String], fields:Option[String])
PATCH     /questionset/v4/update/:identifier         assessment.controllers.v4.QuestionSetController.update(identifier:String)
POST      /questionset/v4/review/:identifier         assessment.controllers.v4.QuestionSetController.review(identifier:String)
POST      /questionset/v4/publish/:identifier        assessment.controllers.v4.QuestionSetController.publish(identifier:String)
DELETE    /questionset/v4/retire/:identifier         assessment.controllers.v4.QuestionSetController.retire(identifier:String)
PATCH     /questionset/v4/add                        assessment.controllers.v4.QuestionSetController.add()
DELETE    /questionset/v4/remove                     assessment.controllers.v4.QuestionSetController.remove()
PATCH     /questionset/v4/hierarchy/update           assessment.controllers.v4.QuestionSetController.updateHierarchy()
GET       /questionset/v4/hierarchy/:identifier      assessment.controllers.v4.QuestionSetController.getHierarchy(identifier:String, mode:Option[String])
POST      /questionset/v4/reject/:identifier         assessment.controllers.v4.QuestionSetController.reject(identifier:String)
POST      /questionset/v4/import                     assessment.controllers.v4.QuestionSetController.importQuestionSet()
PATCH     /questionset/v4/system/update/:identifier  assessment.controllers.v4.QuestionSetController.systemUpdate(identifier:String)
POST      /questionset/v4/copy/:identifier           assessment.controllers.v4.QuestionSetController.copy(identifier:String, mode:Option[String], type:String?="deep")
PATCH     /questionset/v4/comment/update/:identifier assessment.controllers.v4.QuestionSetController.updateComment(identifier:String)
GET       /questionset/v4/comment/read/:identifier   assessment.controllers.v4.QuestionSetController.readComment(identifier:String)


