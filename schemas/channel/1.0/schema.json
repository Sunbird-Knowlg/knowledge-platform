{
  "$id": "channel-schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Channel",
  "type": "object",
  "required": [
    "name",
    "code"
  ],
  "properties": {
    "name": {
      "type": "string"
    },
    "code": {
      "type": "string"
    },
    "description":{
      "type": "string"
    },
    "status":{
      "type": "string",
      "enum": [
        "Draft",
        "Live",
        "Retired"
      ],
      "default": "Live"
    },
    "defaultFramework":{
      "type": "string",
      "default": "NCF"
    },
    "createdBy":{
      "type": "string"
    },
    "createdOn":{
      "type": "string"
    },
    "lastUpdatedBy":{
      "type": "string"
    },
    "lastUpdatedOn":{
      "type": "string"
    },
    "contentFilter":{
      "type": "string"
    },
    "appId":{
      "type": "string"
    },
    "consumerId":{
      "type": "string"
    },
    "lastStatusChangedOn":{
      "type": "string"
    },
    "versionKey":{
      "type": "string"
    },
    "defaultLicense":{
      "type": "string"
    }
  }
}
