{
    "$id": "objectcategory-schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ObjectCategory",
    "type": "object",
    "required": [
        "name"
    ],
    "properties": {
        "name": {
            "type": "string",
            "minLength": 5
        },
        "description": {
            "type": "string"
        },
        "identifier": {
            "type": "string"
        },
        "translations": {
            "type": "object"
        },
        "status": {
            "type": "string",
            "enum": [
                "Live"
            ],
            "default": "Live"
        }
    },
    "additionalProperties": true
}