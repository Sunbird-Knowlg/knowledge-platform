# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Shared Health Endpoints
GET		  /health						    controllers.HealthController.health()
GET       /service/health                   controllers.HealthController.serviceHealth()

# Content Service APIs
POST      /content/v3/create                controllers.content.v3.ContentController.create()
PATCH     /content/v3/update/:identifier    controllers.content.v3.ContentController.update(identifier:String)
GET       /content/v3/read/:identifier      controllers.content.v3.ContentController.read(identifier:String, mode:Option[String], fields:Option[String])
POST      /content/v3/upload/url/:identifier     controllers.content.v3.ContentController.uploadPreSigned(identifier:String, type: Option[String])
POST      /content/v3/upload/:identifier         controllers.content.v3.ContentController.upload(identifier:String, fileFormat: Option[String], validation: Option[String])
POST      /content/v3/copy/:identifier           controllers.content.v3.ContentController.copy(identifier:String, mode:Option[String], type:String ?= "deep")
POST      /content/v3/dialcode/link               controllers.content.v3.ContentController.linkDialCode()
POST      /content/v3/import                 controllers.content.v3.ContentController.importContent()
POST      /content/v3/flag/:identifier           controllers.content.v3.ContentController.flag(identifier:String)
POST      /content/v3/bundle                     controllers.content.v3.ContentController.bundle()
POST      /content/v3/flag/accept/:identifier    controllers.content.v3.ContentController.acceptFlag(identifier:String)
POST      /content/v3/flag/reject/:identifier    controllers.content.v3.ContentController.rejectFlag(identifier:String)
POST      /content/v3/publish/:identifier        controllers.content.v3.ContentController.publish(identifier:String)
POST      /content/v3/review/:identifier         controllers.content.v3.ContentController.review(identifier:String)
DELETE    /content/v3/discard/:identifier        controllers.content.v3.ContentController.discard(identifier:String)
DELETE    /content/v3/retire/:identifier         controllers.content.v3.ContentController.retire(identifier:String)
POST      /content/v3/dialcode/reserve/:identifier controllers.content.v3.ContentController.reserveDialCode(identifier:String)
PATCH     /content/v3/dialcode/release/:identifier controllers.content.v3.ContentController.releaseDialcodes(identifier:String)
POST      /content/v3/reject/:identifier           controllers.content.v4.ContentController.reviewReject(identifier:String)
POST      /content/v3/unlisted/publish/:identifier  controllers.content.v3.ContentController.publishUnlisted(identifier:String)
GET       /content/v3/process/status/:processid     controllers.content.v3.ContentController.getProcessIdStatus(processid: String)

