curl --location --request PATCH 'http://localhost:8080/learning-service/framework/v3/category/master/update/board' \
--header 'Content-Type: application/json' \
--data-raw '{
    "request": {
        "category": {
            "targetIdFieldName": "targetBoardIds",
            "searchLabelFieldName": "se_boards",
            "searchIdFieldName": "se_boardIds",
            "orgIdFieldName": "boardIds"
        }
    }
}'

